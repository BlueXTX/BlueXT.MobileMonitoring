{
  "App": {
    "CorsOrigins": "https://*.MobileMonitoring.com,http://localhost:4200,http://localhost:8082"
  },
  "ConnectionStrings": {
    "Default": "Host=postgres-db;Port=5432;Database=MobileMonitoring;User ID=postgres;Password=postgres;"
  },
  "Redis": {
    "Configuration": "redis"
  },
  "AuthServer": {
    "Authority": "https://localhost:44323",
    "RequireHttpsMetadata": "true",
    "SwaggerClientId": "MobileMonitoring_Swagger"
  },
  "StringEncryption": {
    "DefaultPassPhrase": "ioQafR7bw43s5wRi"
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.File",
      "Serilog.Sinks.Async"
    ],
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "Async",
        "Args": {
          "configure": [
            {
              "Name": "Console"
            },
            {
              "Name": "File",
              "Args": {
                "path": "%TEMP%/Logs/BlueXT.MobileMonitoring.HttpApi.Host/log.txt"
              }
            }
          ]
        }
      },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "seq:5341"
        }
      }
    ],
    "Enrich": [
      "FromLogContext"
    ]
  }
}
